#include <iostream>
#include <string>

using namespace std;

int main(){
    int x, i;
    char c;
    int a_counter = 0;
    int b_counter = 0;
    cin>>x;

    while(x--){
        i = 5;
        while(i--)
        {
            cin>>c;
            c == 'A' ? a_counter++ : b_counter++;
        }
        a_counter > b_counter ? cout<<'A'<<endl : cout<<"B"<<endl;
        a_counter = b_counter = 0;
    }
    cout<<endl;
    return 0;
}
